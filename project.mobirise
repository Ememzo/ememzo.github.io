            "& when (@bg-type = \"color\")": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = \"image\")": {
              "background-image": "url(@bg-value)"
            },
            ".container-fluid": {
              "padding": "0 2rem"
            },
            ".underline": {
              ".line": {
                "width": "80px",
                "height": "4px",
                "background-color": "@lineColor",
                "display": "inline-block"
              }
            },
            ".timeline-text-content": {
              "padding": "2rem 2.5rem",
              "margin-left": "2rem",
              "text-align": "left",
              "p": {
                "margin-bottom": "0"
              }
            },
            ".time-line-date-content": {
              "margin-right": "2rem",
              "p": {
                "padding": "2rem 1.4rem",
                "float": "right"
              }
            },
            ".timeline-element": {
              "margin-bottom": "50px",
              "position": "relative",
              "word-wrap": "break-word",
              "word-break": "break-word",
              "display": "-webkit-flex",
              "-webkit-flex-direction": "row",
              "flex-direction": "row"
            },
            ".iconBackground": {
              "position": "absolute",
              "left": "25%",
              "width": "20px",
              "height": "20px",
              "line-height": "30px",
              "text-align": "center",
              "border-radius": "50%",
              "border": "2px solid @timelineColor",
              "font-size": "30px",
              "display": "inline-block",
              "top": "29px",
              "margin-left": "-10px"
            },
            ".separline:before, .first-separline:before": {
              "top": "3rem",
              "bottom": "0",
              "position": "absolute",
              "content": "\"\"",
              "width": "2px",
              "background-color": "@timelineColor",
              "left": "calc(25% ~\"-\" 1px)",
              "height": "calc(100% ~\"+\" 2rem)"
            },
            ".separline:after": {
              "top": "calc(100% ~\"+\" 6.1rem)",
              "bottom": "0",
              "position": "absolute",
              "content": "\"\"",
              "width": "2px",
              "background-color": "@timelineColor",
              "left": "calc(25% ~\"-\" 1px)",
              "height": "100%"
            },
            "@media (max-width: 768px)": {
              ".container-fluid": {
                "padding": "0 1rem"
              },
              ".iconBackground": {
                "left": "10px !important",
                "top": "33px"
              },
              ".separline:before, .first-separline:before": {
                "left": "9px !important",
                "top": "3.3rem"
              },
              ".separline:after": {
                "left": "9px !important",
                "top": "calc(100% ~\"+\" 6.4rem)"
              },
              ".timeline-text-content": {
                "margin-left": "0 !important"
              },
              ".time-line-date-content": {
                "margin-right": "0 !important",
                "p": {
                  "float": "left !important"
                }
              },
              ".mbr-timeline-date": {
                "padding-bottom": "0 !important"
              }
            },
            ".mbr-section-title, .underline": {
              "text-align": "center"
            },
            ".reverseTimeline": {
              "display": "flex",
              "flex-direction": "column-reverse"
            }
          },
